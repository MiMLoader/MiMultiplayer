    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.2.0",
    "elysia": "^1.2.11",
    "prom-client": "^15.1.3"
  }
}